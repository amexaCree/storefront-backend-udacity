{
  "name": "storefront_backend",
  "version": "1.0.0",
  "description": "A node express server application providing restful api endpoints to manage products and user orders for an online store",
  "main": "server.ts",
  "scripts": {
    "start": "node ./dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test": "set ENV=test&& db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "build": "tsc"
  },
  "author": "Amy Turnah",
  "license": "MIT",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.10.1",
    "@types/pg": "^8.6.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.9.1",
    "jasmine": "^3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "tsc-watch": "^4.2.9"
  }
}
